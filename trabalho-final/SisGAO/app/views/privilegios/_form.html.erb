
<%= simple_form_for @privilegio do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tipo %>
    <%= f.input :descricao %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, f.object.new_record? ? 'Criar Privilegio' : 'Atualizar Privilegio', class: 'btn btn-primary btn-lg' %>
  </div>
<% end %>
