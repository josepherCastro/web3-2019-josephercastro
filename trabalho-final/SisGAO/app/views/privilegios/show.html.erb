<% content_for :title do %>
  Detalhes de Privilegio
<% end %>

<div class="page-header">
  <h1>Detalhes de Privilegio</h1>
  <div>
    <%= link_to edit_privilegio_path(@privilegio), class: 'btn btn-success' do %>
      <%= icon 'fas', 'edit' %>
      Editar
    <% end %>
    <%= link_to privilegios_path, class: 'btn btn-outline-secondary' do %>
      <%= icon 'fas', 'bars' %>
      Voltar
    <% end %>
  </div>
</div>

<dl class="dl-horizontal">
  <dt>Tipo:</dt>
  <dd><%= @privilegio.tipo %></dd>

  <dt>Descricao:</dt>
  <dd><%= @privilegio.descricao %></dd>

</dl>
