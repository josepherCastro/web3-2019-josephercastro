<% content_for :title do %>
  Detalhes de Retirada
<% end %>

<div class="page-header">
  <h1>Detalhes de Retirada</h1>
  <div>
    <%= link_to edit_retirada_path(@retirada), class: 'btn btn-success' do %>
      <%= icon 'fas', 'edit' %>
      Editar
    <% end %>
    <%= link_to retiradas_path, class: 'btn btn-outline-secondary' do %>
      <%= icon 'fas', 'bars' %>
      Voltar
    <% end %>
  </div>
</div>

<dl class="dl-horizontal">
  <dt>Quantidade:</dt>
  <dd><%= @retirada.quantidade %></dd>

  <dt>Material:</dt>
  <dd><%= @retirada.Material %></dd>

  <dt>Funcionario:</dt>
  <dd><%= @retirada.Funcionario %></dd>

</dl>
