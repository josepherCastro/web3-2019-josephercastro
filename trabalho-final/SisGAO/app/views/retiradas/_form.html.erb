
<%= simple_form_for @retirada do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :quantidade %>
    <%= f.association :Material %>
    <%= f.association :Funcionario %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, f.object.new_record? ? 'Criar Retirada' : 'Atualizar Retirada', class: 'btn btn-primary btn-lg' %>
  </div>
<% end %>
