
<%= simple_form_for @devolucao do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :quantidade %>
    <%= f.association :Retirada %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, f.object.new_record? ? 'Criar Devolucao' : 'Atualizar Devolucao', class: 'btn btn-primary btn-lg' %>
  </div>
<% end %>
